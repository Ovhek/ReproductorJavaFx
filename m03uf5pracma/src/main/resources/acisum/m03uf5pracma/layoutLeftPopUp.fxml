<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" maxHeight="-Infinity" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="acisum.m03uf5pracma.LayoutLeftPopUpController">
    <stylesheets>
        <URL value="@/styles/layoutleftpopup.css" />
    </stylesheets>
   <children>
      <VBox prefWidth="100.0">
         <children>
            <TextField fx:id="txt_nombre_lista" />
            <Button fx:id="btn_guardar" mnemonicParsing="false" onAction="#onActionGuardar" text="Guardar">
               <VBox.margin>
                  <Insets left="24.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btn_cancelar" mnemonicParsing="false" onAction="#onActionCancelar" text="Cancelar">
               <VBox.margin>
                  <Insets left="24.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
