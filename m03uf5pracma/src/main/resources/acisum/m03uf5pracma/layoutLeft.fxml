<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import java.net.URL?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="acisum.m03uf5pracma.LayoutLeftController">
    <stylesheets>
        <URL value="@/styles/layout_left.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="200.0" prefWidth="164.0">
         <children>
            <TextField />
            <ListView id="lv_playlists" prefHeight="200.0" prefWidth="200.0" />
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-spacing: 40px;">
               <children>
                  <Button fx:id="btn_add_playlist" mnemonicParsing="false">
                     <graphic>
                        <FontAwesomeIconView glyphName="FOLDER" />
                     </graphic>
                  </Button>
                  <Button fx:id="btn_delete_playlist" mnemonicParsing="false">
                     <graphic>
                        <FontAwesomeIconView text="" />
                     </graphic>
                  </Button>
                  <Button fx:id="btn_filter_playlist" mnemonicParsing="false">
                     <graphic>
                        <FontAwesomeIconView glyphName="TRASH" text="" />
                     </graphic>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
